
range R = 0..2;
dvar float x[R] in 0..400000;


maximize
 ( 20*x[0] - 20*x[0]^2 + 50*x[1] - 50*x[1]^2 + 100*x[2] - 30*x[2]^2 + 18*x[0]*x[2] + 30*x[0]*x[2]  );

subject to {
  ct1:  x[0] +     x[1] + x[2]  <= 60;
  ct2:  x[0] +     5*x[1] + x[2]  <= 40;
  ct3:  x[0] >= 0;
  ct4:  x[1] >= 0;
  ct5:  x[2] >= 0;
}

